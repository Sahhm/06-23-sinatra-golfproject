Remove a Golfer

<form action="/remove_golfer">
  <% Golfer.all_as_objects.each do |club| %>
    <%if session["id"] == nil%>
      <a href="/login_form">Log in first and try again</a>
    <%elsif club.id == session["id"]%>
      <select name="golfer_id">
      <option value="<%= club.id %>"><%= club.name %></option>
      </select><br> 
      <input type="submit">
    <% end %>  
  <% end %>      
</form>